{
  "version": "0.2.0",
  "configurations": [{
    "type": "chrome",
    "request": "launch",
    "port": 9222,
    "name": "Launch Chrome",
    "url": "http://localhost:8080",
    "webRoot": "${workspaceRoot}/src",
    "sourceMaps": true,
    "sourceMapPathOverrides": {
      "webpack:///./src/*": "${webRoot}/*"
    },
  }]
}
